{
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 1,
    "import/no-extraneous-dependencies": 1,
    "no-await-in-loop": 1
  },
  "globals": {"$": true},
  "settings": {
    "import/resolver": "webpack"
  }
}
